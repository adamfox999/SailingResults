# Copy this file to `.env` and fill in the real credentials.
# Values left blank will fall back to the CSV files.

# === Supabase REST API (used by the backend at runtime) ===
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
# Optional overrides
SUPABASE_HANDICAPS_TABLE=handicaps
SUPABASE_SCHEMA=public
SUPABASE_SERIES_TABLE=series
SUPABASE_RACES_TABLE=races
SUPABASE_ENTRIES_TABLE=entries
SUPABASE_SCHEDULE_TABLE=scheduled_races